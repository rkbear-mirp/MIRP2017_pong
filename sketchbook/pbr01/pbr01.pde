int displayWidth = 1000;
int displayHeight = 900;
void setup()
{
size(displayWidth, displayHeight);
}
void draw()
{
float r=90;
background(255, 255, 255);
for(int i=5;i>=1;i--)
{
fill(i*15,0,0);
ellipse(width/2, height/2, i*r+60, i*r+60);
fill(0,i*15,0);
ellipse(width/2, height/2, i*r+30, i*r+30);
fill(0,0,i*15);
ellipse(width/2, height/2, i*r, i*r);
}
}
